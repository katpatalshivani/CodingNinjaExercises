   "source": [
    "def sod(n):\n",
    "   # l=len(n)\n",
    "    if n==0:\n",
    "        return 0\n",
    "    \n",
    "    return n%10+sod(int(n//10))\n",
    "\n",
    "no=int(input())\n",
    "ans=sod(no)\n",
    "print(ans)"
   ]
  }
