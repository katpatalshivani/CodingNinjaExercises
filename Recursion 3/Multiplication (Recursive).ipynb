   "source": [
    "def rmultiply(m,n):\n",
    "    if m==0 or n==0:\n",
    "        return 0\n",
    "    \n",
    "    if m<0 and n<0:\n",
    "        return rmultiply(m,n+1)+m\n",
    "    else:\n",
    "        return rmultiply(m,n-1)+m\n",
    "\n",
    "n1=int(input())\n",
    "n2=int(input())\n",
    "print(rmultiply(n1,n2))"
   ]
