    "count=0\n",
    "def countZ(num):\n",
    "    #count =0\n",
    "    if num ==0:\n",
    "        return 1\n",
    "    global count\n",
    "    if num > 0:\n",
    "        if(num%10 == 0):\n",
    "            count=count +1\n",
    "            \n",
    "        countZ(num//10)\n",
    "        \n",
    "    return count\n",
    "\n",
    "n=int(input())\n",
    "ans= countZ(n)\n",
    "print(ans)"
   ]
