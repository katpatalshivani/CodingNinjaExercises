    "k=2\n",
    "n=len(arr)#after k>5, the test case will be iterating\n",
    "for i in range(k%n):#10 again array will be same as 5\n",
    "    #in one rotation \n",
    "    curr = arr[0]\n",
    "    \n",
    "    for j in range(1,n):\n",
    "        arr[j-1]=arr[j]\n",
    "        \n",
    "    arr[n-1]=curr\n",
    "    \n",
    "print(arr)"
   ]
